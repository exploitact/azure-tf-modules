variable "b2c_display_name" {
  description = "B2C display name"
  type        = string
  default     = "sample_display"
}

variable "b2c_domain_name" {
  description = "B2C domain name"
  type        = string
  default     = "domain_name"
}

variable "b2c_resource_group" {
  description = "B2C resource group name"
  type        = string
  default     = "sample"
}

variable "b2c_sku_name" {
  description = "B2C sku name"
  type        = string
  default     = "PremiumP1"
}